{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vue2-vuetify-ts-starter",
  "description": "Vue2 Vuetify TypeScript Startar project for Vite.",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vite-vue2-vuetify-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vite-vue2-vuetify-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vite-vue2-vuetify-ts-starter/issues"
  },
  "engines": {
    "node": ">=18.12.0",
    "yarn": ">=1.22.10"
  },
  "packageManager": "yarn@3.2.4",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint  && prettier  \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "test": "vitest",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:coverage": "vitest run --coverage",
    "build": "run-p type-check build-only",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "build-only": "vite build",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@logue/vue2-helpers": "^2.1.1",
    "@mdi/font": "^7.0.96",
    "vue": "^2.7.13",
    "vue-router": "^3.6.5",
    "vuetify": "^2.6.12",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.11.9",
    "@types/webfontloader": "^1.6.35",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-vue2": "^2.0.0",
    "@vitest/coverage-c8": "^0.24.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.26.0",
    "eslint-config-google": "^0.14.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.5.0",
    "eslint-plugin-jsonc": "^2.5.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.7.0",
    "eslint-plugin-vuejs-accessibility": "^2.0.0",
    "eslint-plugin-vuetify": "^1.1.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "sass": "1.32.12",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.8.4",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.2",
    "vite-plugin-checker": "^0.5.1",
    "vitest": "^0.24.5",
    "vue-eslint-parser": "^9.1.0",
    "vue-template-compiler": "^2.7.13",
    "vue-tsc": "^1.0.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.vue": "vue-tsc --noEmit",
    "*.{js,ts,json,jsonc,json5,yaml,yml,toml,yml,yaml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "prettier": "^2.7.1"
  },
  "stackblitz": {
    "startCommand": "yarn run test"
  }
}
